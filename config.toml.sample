allowed_domains = []
auto_update = 168 # 1 week
debug = false
external = true
ip = "192.168.0.42"
filters_path = "./"
filter_version = "OneHostsPro"
small = true
verbosity = 0
#web_password = "superpassword"

[overrides]
"example.com" = [4, 3, 2, 1]

# Network-specific overrides - these take precedence over default overrides
# Format: [network_overrides."CIDR_RANGE"]
# Example: Give different responses to VPN clients vs regular network clients
[network_overrides."100.64.0.0/10"]
"internal.example.com" = [10, 0, 0, 1]
"api.example.com" = [192, 168, 1, 50]

[network_overrides."192.168.1.0/24"]
"printer.local" = [192, 168, 1, 100]
